# Weaviate API key for authentication (REQUIRED - docker-compose will fail without it)
WEAVIATE_API_KEY=your-secret-key-here
WEAVIATE_API_USER=admin@varity.local

# Varity search service API key (optional; if set, all /search requests require it)
VARITY_API_KEY=

# Weaviate connection
WEAVIATE_URL=http://weaviate:8080

# CORS allowed origins (default: http://localhost:8000)
# Set to "*" only for development; restrict in production
VARITY_CORS_ORIGINS=http://localhost:8000
